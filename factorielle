def factorielle1(n):
    assert n>=0, "factorielle inexistante"
    produit = 1
    for i in range(1,n+1):
        produit *= i
    return produit

def factorielle2(n):
    assert n>=0, "factorielle inexistante"
    if n==0:
        return 1
    return n*factorielle2(n-1)

for i in range(20):
    print(f"{i}! (iterative) = {factorielle1(i)}, (recursive) = {factorielle2(i)}")
